# Clerk Authentication Configuration
# Copy this file to .env.local and add your Clerk publishable key
# Get your key from: https://dashboard.clerk.com

NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_key_here

# Unity Integration Configuration
# Custom URL scheme for Unity deep linking
# Examples: unity://, dreamforge://, myapp://
# For web-based Unity builds, use a full URL like: https://your-unity-app.com/auth
NEXT_PUBLIC_UNITY_URL_SCHEME=unity://

